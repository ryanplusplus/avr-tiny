      "executable": "./build/tiny.elf",
      "request": "launch",
      "servertype": "openocd",
      "configFiles": [
        ""
      ],
      "serverpath": "./build/debug-server-dwdebug",
      "overrideLaunchCommands": [
        "file ./build/tiny.elf",
        "load"
      ],
      "toolchainPrefix": "avr",
      "preLaunchTask": "debug-deps",
      "runToMain": true
